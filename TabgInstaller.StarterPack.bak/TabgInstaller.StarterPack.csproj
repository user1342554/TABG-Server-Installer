    </Reference>
    <Reference Include="BepInEx">
      <HintPath>..\TabgInstaller.AntiCheatBypass\Libs\BepInEx.dll</HintPath>
    </Reference>
    <Reference Include="UnityEngine.CoreModule">
      <HintPath>..\TabgInstaller.AntiCheatBypass\Libs\UnityEngine.CoreModule.dll</HintPath>
    </Reference>
    <Reference Include="UnityEngine">
      <HintPath>..\TabgInstaller.AntiCheatBypass\Libs\UnityEngine.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project> 