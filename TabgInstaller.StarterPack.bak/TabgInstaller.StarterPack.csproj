    <AssemblyName>TabgInstaller.StarterPack</AssemblyName>
    <Description>StarterPack mod integrated into TabgInstaller - comprehensive TABG server configuration</Description>
    <Version>2.1.0</Version>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

  <ItemGroup>
